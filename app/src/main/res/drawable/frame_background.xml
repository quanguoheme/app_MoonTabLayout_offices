<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <layer-list
          xmlns:android="http://schemas.android.com/apk/res/android">
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="#ff8b8682" />
                    <corners android:topLeftRadius="10.0dip" android:topRightRadius="10.0dip" android:bottomLeftRadius="10.0dip" android:bottomRightRadius="10.0dip" />
                </shape>
            </item>
            <item android:left="1.0px" android:top="1.0px" android:right="1.0px" android:bottom="1.0px">
                <shape>
                    <gradient android:startColor="#ffefefef" android:endColor="#ffefefef" android:angle="45.0" android:type="linear" android:centerX="0.5" android:centerY="0.5" />
                    <corners android:topLeftRadius="5.0dip" android:topRightRadius="5.0dip" android:bottomLeftRadius="5.0dip" android:bottomRightRadius="5.0dip" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_pressed="false">
        <layer-list
          xmlns:android="http://schemas.android.com/apk/res/android">
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="#ff8b8682" />
                    <corners android:topLeftRadius="5.0dip" android:topRightRadius="5.0dip" android:bottomLeftRadius="5.0dip" android:bottomRightRadius="5.0dip" />
                </shape>
            </item>
            <item android:left="1.0px" android:top="1.0px" android:right="1.0px" android:bottom="1.0px">
                <shape>
                    <gradient android:startColor="#ffefefef" android:endColor="#ffefefef" android:angle="45.0" android:type="linear" android:centerX="0.5" android:centerY="0.5" />
                    <corners android:topLeftRadius="5.0dip" android:topRightRadius="5.0dip" android:bottomLeftRadius="5.0dip" android:bottomRightRadius="5.0dip" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>